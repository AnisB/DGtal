// ----------------------- Standard services ------------------------------


template <typename TImage>
inline
DGtal::DifferentialOperatorsOnImages<TImage>
::DifferentialOperatorsOnImages( Image& aStartingImage )
: myU( aStartingImage ), myH( 1 )
{
}


template <typename TImage>
inline
DGtal::DifferentialOperatorsOnImages<TImage>::~DifferentialOperatorsOnImages()
{
}




template <typename TImage>
inline
void
DGtal::DifferentialOperatorsOnImages<TImage>::selfDisplay ( std::ostream & out ) const
{
  out << "[DifferentialOperatorsOnImages]\n";
  out << "\n";
}

template <typename TImage>
inline
bool
DGtal::DifferentialOperatorsOnImages<TImage>::isValid() const
{
    return true;
}



///////////////////////////////////////////////////////////////////////////////
// Implementation of inline functions                                        //

template <typename TImage>
inline
std::ostream&
DGtal::operator<< ( std::ostream & out, 
		  const DifferentialOperatorsOnImages<TImage> & object )
{
  object.selfDisplay( out );
  return out;
}

